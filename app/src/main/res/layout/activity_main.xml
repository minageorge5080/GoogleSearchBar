<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <com.mina.googlesearchbar.GoogleSearchBar
            android:id="@+id/googleSearchBar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:GoogleSB_et_hint="Searchhhh"
            app:GoogleSB_et_elevation="20dp"
            app:GoogleSB_et_textSize="8sp"
            app:GoogleSB_et_marginRight="20dp"
            app:GoogleSB_et_marginLeft="20dp"
            app:GoogleSB_et_marginTop="20dp"
            app:GoogleSB_et_paddingLeft="10dp"
            app:GoogleSB_et_paddingRight="10dp"
            app:GoogleSB_et_paddingTop="10dp"
            app:GoogleSB_et_paddingBottom="10dp"
            app:GoogleSB_et_text_color="@color/colorWhite"
            app:GoogleSB_et_background="@drawable/shape_google_search_bg"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>


